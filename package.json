{
  "name": "codememo",
  "displayName": "codeMemo",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.64.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:codememo.helloWorld",
    "onCommand:codememo.webview"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codememo.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "codememo.webview",
        "title": "Webview"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js, jsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.64.0",
    "@vscode/test-electron": "^2.1.2",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "glob": "^7.2.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.3.4",
    "mocha": "^9.2.1",
    "typescript": "^4.5.5"
  }
}
